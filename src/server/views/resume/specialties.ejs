<% if (resume.specialties.length > 0) { %>
<div class="resume-specialties">

    <h2>Specialties</h2>

    <div class="resume-specialties-list" >
        <% resume.specialties.forEach(function(specialty) { %>
            <div class="resume-specialty">
                <i class="fa-solid fa-check"></i>
                <%= specialty %>
            </div>
        <% }); %>
    </div>
</div>
<% } %>
